<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE configuration PUBLIC
"-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>

	<typeAliases>		
		<typeAlias type="com.dal.likeycakey.member.model.vo.Member" alias="Member"/>
		<typeAlias type="com.dal.likeycakey.biz.model.vo.BizMember" alias="bizMember"/>
		<typeAlias type="com.dal.likeycakey.detailView.model.vo.ProductBoard" alias="ProductBoard"/>
		<!-- <typeAlias type="org.kh.firstSpring.notice.model.vo.Notice" alias="Notice"/>
		<typeAlias type="org.kh.firstSpring.board.model.vo.Board" alias="Board"/> -->
	</typeAliases> 	
	

	<!-- 마이바티스 DataSource 설정은 root-context.xml을 참조한다. -->
	
	<mappers>
		<mapper resource="mappers/member-mapper.xml"/>
		<mapper resource="mappers/bizmember-mapper.xml"/>
		<mapper resource="mappers/productboard-mapper.xml"/>
		<!-- <mapper resource="mappers/notice-mapper.xml"/>
		 -->
	</mappers>

</configuration>